{
    "name": "jiny/database",
    "description": "database for JinyPHP",
    "keywords": [
        "jinyPHP",
        "framwork",
        "database"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\Database\\": "/"
        },
        "files": [
            "Helpers/Helper.php"
        ]

    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\Database\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\Database\\JinyDatabaseServiceProvider"
            ]
        }
    }
}
